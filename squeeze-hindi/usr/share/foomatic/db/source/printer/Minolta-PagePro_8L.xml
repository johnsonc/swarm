<printer id="printer/Minolta-PagePro_8L">
  <make>Minolta</make>
  <model>PagePro 8L</model>
  <mechanism>
    <laser />
    <resolution>
      <dpi>
        <x>600</x>
        <y>600</y>
      </dpi>
    </resolution>
  </mechanism>
  <url>http://www.minolta.de/beo/office/produkte/drucker/ppro8l/ppro8l_subframe.htm</url>
  <lang>
    <pcl level="4.5" />
    <pjl />
