<driver id="driver/okiibm">
  <name>okiibm</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=okiibm%A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Oki-Microline_IBM_compatible_9_pin</id>
    </printer>
    <printer>
      <id>printer/Oki-ML_320</id>
    </printer>
    <printer>
      <id>printer/Oki-ML_321</id>
    </printer>
  </printers>
</driver>
