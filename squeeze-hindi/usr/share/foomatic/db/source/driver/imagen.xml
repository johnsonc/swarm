<driver id="driver/imagen">
  <name>imagen</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=imagen%A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Imagen-ImPress</id>
    </printer>
  </printers>
</driver>
