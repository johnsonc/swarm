<driver id="driver/cp50">
  <name>cp50</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=cp50%A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Mitsubishi-CP50_Color_Printer</id>
    </printer>
  </printers>
</driver>
