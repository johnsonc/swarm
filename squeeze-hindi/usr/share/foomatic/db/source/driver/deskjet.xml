<driver id="driver/deskjet">
  <name>deskjet</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=deskjet%A%Z -sOutputFile=-%C -f -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/HP-DeskJet</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_200</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_340C</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_Plus</id>
    </printer>
    <printer>
      <id>printer/HP-OfficeJet_R45</id>
    </printer>
  </printers>
</driver>
