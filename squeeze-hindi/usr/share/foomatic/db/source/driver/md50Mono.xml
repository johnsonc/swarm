<driver id="driver/md50Mono">
  <name>md50Mono</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=md50Mono%A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Alps-MD-5000</id>
    </printer>
    <printer>
      <id>printer/Oki-DP_5000</id>
    </printer>
  </printers>
</driver>
