<driver id="driver/djet500">
  <name>djet500</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=djet500%A%Z -sOutputFile=-%C -f -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/DEC-DECwriter_110i</id>
    </printer>
    <printer>
      <id>printer/DEC-DECWriter_500i</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_320</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_500</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_510</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_520</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_540</id>
      <comments>
        <en>For black-only printing.</en>
      </comments>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_540C</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_550C</id>
    </printer>
    <printer>
      <id>printer/HP-DeskJet_600</id>
      <comments>
        <en>For mono.</en>
      </comments>
    </printer>
    <printer>
      <id>printer/HP-OfficeJet</id>
    </printer>
    <printer>
      <id>printer/HP-OfficeJet_300</id>
    </printer>
    <printer>
      <id>printer/HP-OfficeJet_330</id>
    </printer>
    <printer>
      <id>printer/HP-OfficeJet_350</id>
    </printer>
    <printer>
      <id>printer/HP-OfficeJet_LX</id>
    </printer>
    <printer>
      <id>printer/Olivetti-JP450</id>
    </printer>
  </printers>
</driver>
