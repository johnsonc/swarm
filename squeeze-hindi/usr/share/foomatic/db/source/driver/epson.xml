<driver id="driver/epson">
  <name>epson</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=epson%A%Z -sOutputFile=- -</prototype>
  </execution>
