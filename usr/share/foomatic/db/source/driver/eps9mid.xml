<driver id="driver/eps9mid">
  <name>eps9mid</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=eps9mid%A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Epson-Dot_Matrix</id>
    </printer>
    <printer>
      <id>printer/Generic-ESC_P_Dot_Matrix_Printer</id>
    </printer>
    <printer>
      <id>printer/QMS-LPK-100</id>
    </printer>
    <printer>
      <id>printer/Star-NX-1001</id>
    </printer>
  </printers>
</driver>
