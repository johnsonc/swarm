<driver id="driver/lq850">
  <name>lq850</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=lq850%A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Brother-HJ-400</id>
    </printer>
    <printer>
      <id>printer/Canon-BJ-300</id>
    </printer>
    <printer>
      <id>printer/Epson-LQ-850</id>
    </printer>
    <printer>
      <id>printer/Generic-ESC_P_Dot_Matrix_Printer</id>
    </printer>
  </printers>
</driver>
