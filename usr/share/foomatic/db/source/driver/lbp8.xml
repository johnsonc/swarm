<driver id="driver/lbp8">
  <name>lbp8</name>
  <url>http://www.ghostscript.com/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=lbp8%A%Z -sOutputFile=- -</prototype>
  </execution>
  <printers>
    <printer>
      <id>printer/Canon-LBP-4plus</id>
    </printer>
    <printer>
      <id>printer/Canon-LBP-4U</id>
    </printer>
    <printer>
      <id>printer/Canon-LBP-8A1</id>
      <comments>
        <en>Special-purpose gs driver.  Bound to work.</en>
      </comments>
    </printer>
  </printers>
</driver>
